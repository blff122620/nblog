<ul>
    <% for(var i=0; i<nav.length; i++){ %>
        <% if(nav[i].display) {%>
         <li><a 
           
            <%if(nav[i].href){%>
                href="<%= nav[i].href%>"
            <%}%> 
            class="underline 
                <%if(nav[i].selected){%>
                    selected
                <%}%> 
                <%if(nav[i].addClass){%>
                    <%= nav[i].addClass%>
                <%}%> 
            "
            ><%= nav[i].value%>
         </a></li>
         <% } %>
    <% }%>
    <% if(user) {%>
    <li><a href="/personal/info" class="underline"><%=user.nickname%></a></li>
    <li><a href="/signout" class="underline">登出</a></li>
    <% }%>
</ul>